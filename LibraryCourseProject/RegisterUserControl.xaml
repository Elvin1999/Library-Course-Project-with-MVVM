<UserControl x:Class="LibraryCourseProject.RegisterUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LibraryCourseProject"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Aqua">
        <Label Content="Registration" Margin="50" FontSize="25" FontFamily="Comic">
            
        </Label>

        <StackPanel Orientation="Horizontal">
            <StackPanel Orientation="Vertical" Margin="10,100,0,0">
                <Label Width="100" Height="35" Margin="0" Content="Username" FontSize="18"></Label>
                <Label Width="100" Height="35" Margin="0,5" Content="Email" FontSize="18"></Label>
                <Label Width="100" Height="35" Margin="0,0" Content="Password" FontSize="18"></Label>
                <Label Width="100" Height="35" Margin="0,5" Content="Note" FontSize="18"></Label>
            </StackPanel>
            <StackPanel Orientation="Vertical">
                
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
