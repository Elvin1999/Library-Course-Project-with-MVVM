<Window x:Class="LibraryCourseProject.Views.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryCourseProject.Views"
        mc:Ignorable="d"
        Title="MenuWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Width="120" Height="50" Content="Users" Command="{Binding UsersSectionCommand}"></Button>
            <Button Width="120" Height="50" Content="Filials"></Button>
            <Button Width="120" Height="50" Content="Workers"></Button>
            <Button Width="120" Height="50" Content="Clients"></Button>
            <Button Width="120" Height="50" Content="Book"></Button>
        </StackPanel>
    </Grid>
</Window>
